# Git Push MCP 配置文件示例

# 日志级别 (error, warn, info, verbose, debug, silly)
LOG_LEVEL=info

# 是否启用自动暂存
AUTO_STAGE=true

# 是否启用自动推送
AUTO_PUSH=false

# 是否使用约定式提交格式
CONVENTIONAL_COMMITS=true

# 提交消息最大长度
MAX_COMMIT_LENGTH=72

# 允许空提交
ALLOW_EMPTY_COMMITS=false

# Git用户名（可选，会使用全局配置）
GIT_USER_NAME=

# Git用户邮箱（可选，会使用全局配置）
GIT_USER_EMAIL=